  "homepage": "/smeagol",
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "better-react-spinkit": "^2.0.4",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "ces-theme": "https://github.com/cloudogu/ces-theme.git",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.0.1",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "highlight.js": "^9.12.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "2.29.0",
    "i18next": "^10.3.0",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-fetch-backend": "^0.1.0",
    "jest": "20.0.4",
    "moment": "^2.20.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "query-string": "^5.0.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-i18next": "^7.3.4",
    "react-jss": "^8.2.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "tui-editor": "^1.0.2",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "cross-env HOST=192.168.56.1 node src/main/scripts/start.js",
    "build": "node src/main/scripts/build.js",
    "test": "node src/main/scripts/test.js --env=jsdom",
    "test-mvn": "cross-env CI=true node src/main/scripts/test.js --env=jsdom"
  },
  "proxy": {
    "/smeagol/api": {
      "target": "http://192.168.56.1:8080"
    }
  },
  "devDependencies": {
    "flow-bin": "^0.64.0",
    "jest-junit-reporter": "^1.1.0",
    "redux-devtools-extension": "^2.13.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/src/main/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/main/js/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/main/js/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/src/main/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/src/main/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
